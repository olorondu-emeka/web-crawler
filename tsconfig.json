{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2018",
    "lib": [
      "es2018",
      "dom",
      "ESNext.AsyncIterable"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "checkJs": false,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}